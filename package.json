    "greenlock-express": "^4.0.3",
    "mongoist": "^2.5.3",
    "query-string": "^7.0.0",
    "react": "^16.5.2",
    "react-copy-to-clipboard": "^5.0.3",
    "react-dom": "^16.5.2",
